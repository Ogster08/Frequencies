<Window x:Class="frequencies.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:frequencies" 
        xmlns:usercontrols="clr-namespace:frequencies.View.UserControls"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Rectangle  Fill="#363454"/>
        <Button BorderThickness="0"  Content="Generic" FontSize="30" Background="Transparent" HorizontalAlignment="Center" VerticalAlignment="Top" Click="Button_Click"/>
        <Frame x:Name="Main" NavigationUIVisibility="Hidden" x:FieldModifier="public"/>
    </Grid>
</Window>
